{
  "workbench.colorTheme": "Default Dark+",
  "files.autoSave": "onFocusChange",
  "terminal.integrated.defaultProfile.windows": "Ubuntu",
  "http.proxyStrictSSL": false,


  "window.zoomLevel": 1,
  "autoDocstring.docstringFormat": "google-notypes",
  "workbench.settings.editor": "json",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.formatting.provider": "yapf",
  "python.linting.flake8Args": ["--max--line-length=248"],

  "[python]": {
    "editor.formatOnType": true
  },
  "vsicons.dontShowNewVersionMessage": true,

  "files.associations": {
    "string.h": "c",
    "stdio.h": "c",
    "printutils.h": "c",
    "reverse_polish.h": "c",
    "stack.h": "c",
    "input.h": "c",
    "stdlib.h": "c",
    "limits.h": "c",
    "corecrt_wstdlib.h": "c",
    "vec.h": "c",
    "cstdlib": "c",
    "unistd.h": "c",
    "getopt.h": "c",
    "csapp.h": "c",
    "cctype": "c",
    "array": "c",
    "string_view": "c",
    "inet.h": "c",
    "socket.h": "c",
    "netdb.h": "c",
    "quick_sort.h": "c",
    "pthread.h": "c",
    "thread": "c",
    "*.tcc": "c",
    "functional": "c",
    "tuple": "c",
    "sbuf.h": "c",
    "curl.h": "c",
    "in.h": "c",
    "types.h": "c",
    "user.h": "c",
    "stat.h": "c",
    "fcntl.h": "c",
    "syscall.h": "c",
    "exception": "c",
    "memory_resource": "c",
    "iosfwd": "c",
    "new": "c",
    "ostream": "c",
    "stop_token": "c",
    "streambuf": "c",
    "atomic": "c",
    "fstream": "c",
    "sstream": "c",
    "random": "c",
    "fs.h": "c",
    "param.h": "c",
    "defs.h": "c",
    "proc.h": "c",
    "spinlock.h": "c",
    "riscv.h": "c",
    "memlayout.h": "c"
  },
  "C_Cpp.default.compilerPath": "/usr/bin/gcc"
}
